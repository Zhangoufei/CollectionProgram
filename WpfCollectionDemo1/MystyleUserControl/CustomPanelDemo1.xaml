<Window x:Class="MystyleUserControl.CustomPanelDemo1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MystyleUserControl"
        mc:Ignorable="d"
        Title="CustomPanelDemo1" Height="450" Width="800">
    <Grid>
        <!---使用自定义旋转模板-->
        <local:CustomePanel >
            <Button Content="1" Background="AntiqueWhite" MinWidth="100" />
            <Button Content="2" Background="AntiqueWhite" MinWidth="100" />
            <Button Content="3" Background="AntiqueWhite" MinWidth="100" />
            <Button Content="4" Background="AntiqueWhite" MinWidth="100" />
            <Button Content="5" Background="AntiqueWhite" MinWidth="100" />
            <Button Content="6" Background="AntiqueWhite" MinWidth="100" />
            <Button Content="7" Background="AntiqueWhite" MinWidth="100" />
        </local:CustomePanel>
    </Grid>
</Window>
